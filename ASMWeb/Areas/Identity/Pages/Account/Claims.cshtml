@page
@model ASMWeb.Areas.Identity.Pages.Account.ClaimsModel
 
@{
    ViewData["Title"] = "View User Claims";
}
<h1>@ViewData["Title"]</h1>
<br />
@if (User.Identity.IsAuthenticated)
{
    <table class="table table-responsive-sm">
        <tr><th>Claim Type :</th><th>Claim Value</th><th>Claim Issuwe</th></tr>
        @foreach (var claim in User.Claims)
        {
        <tr>
            <td>  @claim.Type</td>
            <td>  @claim.Value</td>
            <td>  @claim.Issuer</td>
        </tr>
        }
    </table>
}